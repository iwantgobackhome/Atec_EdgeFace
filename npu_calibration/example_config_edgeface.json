{
  "inputs": {
    "input.1": [
      1,
      3,
      112,
      112
    ]
  },
  "calibration_num": 100,
  "calibration_method": "ema",
  "default_loader": {
    "dataset_path": "./calibration_dataset",
    "file_extensions": [
      "jpeg",
      "jpg",
      "png",
      "JPEG",
      "JPG",
      "PNG"
    ],
    "preprocessings": [
      {
        "convertColor": {
          "form": "BGR2RGB"
        }
      },
      {
        "resize": {
          "width": 112,
          "height": 112
        }
      },
      {
        "div": {
          "x": 255.0
        }
      },
      {
        "normalize": {
          "mean": [
            0.5,
            0.5,
            0.5
          ],
          "std": [
            0.5,
            0.5,
            0.5
          ]
        }
      },
      {
        "transpose": {
          "axis": [
            2,
            0,
            1
          ]
        }
      },
      {
        "expandDim": {
          "axis": 0
        }
      }
    ]
  }
}
